container_commands:
    01_migrate:
        command: "source /var/app/venv/*/bin/activate && python3 manage.py migrate"
        leader_only: true
    02_compilemessages:
        command: "source /var/app/venv/*/bin/activate && python3 manage.py compilemessages"
    03_createsu:
        command: "source /var/app/venv/*/bin/activate && cd /var/app/staging ; python3 manage.py createsu"
    04_collectstatic:
        command: "source /var/app/venv/*/bin/activate &&  python3 manage.py collectstatic --noinput"
    05_seedamenities:
        command: "source /var/app/venv/*/bin/activate &&  python3 manage.py seed_amenities "
    06_seedfacilities:
        command: "source /var/app/venv/*/bin/activate &&  python3 manage.py seed_facilities "
    07_seedrules:
        command: "source /var/app/venv/*/bin/activate &&  python3 manage.py seed_rules "
    08_seedrooms:
        command: "source /var/app/venv/*/bin/activate &&  python3 manage.py seed_rooms --number 50 "
option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: config.wsgi:application
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: config.settings